/* index.acss */
.container { padding: 20rpx; text-align: center; }
.title { font-size: 50rpx; font-weight: bold; color: #0c6; margin-bottom: 30rpx; }
.board { margin-top: 40rpx; }
.row { display: flex; justify-content: center; }
.cell {
  width: 100rpx; height: 100rpx;
  border: 1px solid #333; line-height: 100rpx;
  font-size: 40rpx; margin: 5rpx; background: #fff;
}
.buttons button { margin: 10rpx 0; }
.info { margin-top: 30rpx; font-size: 30rpx; }
